{
  "name": "Arxiv Auto Bookmarker",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Automatically bookmarks viewed arxiv papers. Forked from https://github.com/eylonyogev/Where-s-That-Paper-",
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery/jquery.js",
	  "src/bg/bibtexParser.js",
	  "src/bg/scrapers.js",
	  "src/bg/background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
	"bookmarks",
	"tabs"
  ]
}